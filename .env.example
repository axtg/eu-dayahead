# Server Configuration
PORT=3000
NODE_ENV=development

# API Keys
ENTSOE_API_KEY=your-entsoe-api-key-here
LOGTAIL_TOKEN=your-logtail-token-here
LOGTAIL_ENDPOINT=your-logtail-http-endpoint-here

# Cache Configuration
# How long to cache upstream raw price results (ms)
CACHE_TIMEOUT_MS=3600000

# Rate Limiting
# Max requests per IP per second for /api endpoints
RATE_LIMIT_RPS=10

# CORS Configuration
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info

# Optional: Custom API Base URL for documentation
API_BASE_URL=http://localhost:3000
