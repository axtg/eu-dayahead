# Test Environment Configuration
NODE_ENV=test
PORT=3001

# API Keys (use test keys or leave empty for mocking)
ENTSOE_API_KEY=
LOGTAIL_TOKEN=
LOGTAIL_ENDPOINT=

# Cache Configuration (shorter for tests)
CACHE_TIMEOUT_MS=1000

# Rate Limiting (more permissive for tests)
RATE_LIMIT_WINDOW_MS=60000
